def bin_search(arr,target):
    high = len(arr)-1
    low = 0
    while low <= high:
        mid = low + (high - low)//2
        if arr[mid] == target:
            return mid
        elif arr[mid] > target:
            high = mid -1
        else:
            low = mid +1
    return -1
arr = [2,4,5,6,9,13]
print(bin_search(arr, 9))